jQuery(document).ready(function() {
  jQuery('#home-billboard-1 #page-right, .home-billboard-green #page-right').click(function () {
    jQuery(this).parent().hide();
    jQuery(this).parent().next().show();
  });
  jQuery('#home-billboard-1 #page-left').click(function () {
    jQuery(this).parent().hide();
    jQuery('#billboard-5').hide();
    jQuery('#billboard-6').show();
  });
  jQuery('.home-billboard-green #page-left').click(function () {
    jQuery(this).parent().hide();
    jQuery(this).parent().prev().show();
  });
  jQuery('#billboard-5 #page-right').click(function () {
    jQuery(this).parent().hide();
    jQuery('#billboard-6').show();
    jQuery('#home-billboard-1').hide();
  });
  jQuery('#billboard-6 #page-right').click(function () {
    jQuery(this).parent().hide();
    jQuery('#home-billboard-1').show();
  });
  jQuery('.dot-billboard-1').click(function () {
    jQuery('#home-billboard-1').show();
    jQuery('.home-billboard-green').hide();
  });
  jQuery('.dot-billboard-2').click(function () {
    jQuery('#home-billboard-1, #billboard-3, #billboard-4, #billboard-5, #billboard-6').hide();
    jQuery('#billboard-2').show();
  });
  jQuery('.dot-billboard-3').click(function () {
    jQuery('#home-billboard-1, #billboard-2, #billboard-4, #billboard-5, #billboard-6').hide();
    jQuery('#billboard-3').show();
  });
  jQuery('.dot-billboard-4').click(function () {
    jQuery('#home-billboard-1, #billboard-2, #billboard-3, #billboard-5, #billboard-6').hide();
    jQuery('#billboard-4').show();
  });
  jQuery('.dot-billboard-5').click(function () {
    jQuery('#billboard-5').show();
    jQuery('#home-billboard-1, #billboard-2, #billboard-3, #billboard-4, #billboard-6').hide();
  });
  jQuery('.dot-billboard-6').click(function () {
    jQuery('#billboard-6').show();
    jQuery('#home-billboard-1, #billboard-2, #billboard-3, #billboard-4, #billboard-5').hide();
  });
});
</script>

